template <typename T>
class list {
private:
    T val[200];
    T frnt;
public:

    struct const_iterator
    {
        using value_type = T;
        using pointer = T; 
        using reference = T&;



        const_iterator(pointer ptr) : m_ptr(ptr) {}

        pointer operator->() { return m_ptr; }
        reference operator*() {return m_ptr; }

    private:

        pointer m_ptr;
        

    };

    const_iterator begin() { return const_iterator(val[0]); }

    list() {

    }

    void push_back(T a){
      
    }

    T &front() {   
      return frnt;
    }

private:


};