#include <algorithm>
#include <iostream>
#include <initializer_list>
#include <iterator>
using namespace std;

template <typename T>
struct Node{
        T my_data;
        Node* next;
        Node() = default;
        constexpr explicit Node(T d) : my_data(d) {}
        operator T(){
            return my_data;
        }
        operator T() const{
            return my_data;
        }
        T* operator&(){
            return &my_data;
        }
        Node operator++(){
            return next;
        }
    };

template <typename T>
class forward_list{
    private:
        Node<T>* my_before;
        Node<T>* my_head;
        Node<T>* my_tail;
        Node<T>* my_after;
        size_t my_size;

        

    public:
        using value_type            = T;
        using size_type             = size_t;
        //todo difference type
        using reference             = T&; 
        using const_reference       = const T&;
        using pointer               = T*;
        using const_pointer         = const T;
        
        typedef iterator_base<Node<T>>            iterator;
        typedef const iterator_base<Node<T>>      const_iterator;

    forward_list(){
        Node<T>* bef = new Node<T>();
        Node<T>* aft = new Node<T>();
        bef->next = aft;
        my_before = bef;
        my_after = aft;
        my_head = nullptr;
        my_tail = nullptr;
        my_size = 0;
    }

    private:

    void fixup_pointers(){
        if(my_size == 0){
            my_head = nullptr;
            my_tail = nullptr;
            my_before->next = my_after;
            return;
        }
        Node<T>* curr = my_before;
        my_head = curr->next;
        while(curr->next != my_after){
            curr = curr->next;
        }
        my_tail = curr;
    }

    public:

    // ELEMENT ACCESS
    reference front(){
        return my_head->my_data;
    }

    const_reference front() const{
        return my_head->my_data;
    }

    // ITERATORS
    iterator before_begin() noexcept{
        return iterator(my_before);
    }
    const_iterator before_begin() const noexcept{
        return const_iterator(my_before);
    }
    const_iterator cbefore_begin() const noexcept{
        return const_iterator(my_before);
    }

    iterator begin() noexcept{
        return iterator(my_head);
    }
    const_iterator begin() const noexcept{
        return const_iteratorq(my_head);
    }
    const_iterator cbegin() const noexcept{
        return const_iteratorq(my_head);
    }

    iterator end() noexcept{
        return iterator(my_after);
    }
    const_iterator end() const noexcept{
        return const_iterator(my_after);
    }
    const_iterator cend() const noexcept{
        return const_iterator(my_after);
    }

    // CAPACITY
    bool empty() const noexcept{
        return my_size == 0;
    }

    size_type max_size() const noexcept;

    // MODIFIERS
    void clear() noexcept{

        if(my_size == 0){
            return;
        }
        Node<T>* curr = my_head;
        while(curr != my_after){
            Node<T>* temp = curr;
            curr = curr->next;
            delete temp;
        }
        my_before->next = my_after;
        my_head = nullptr;
        my_tail = nullptr;
        my_size = 0;
    }

    iterator insert_after( const_iterator pos, const T& value ){
        T val = value;
        Node<T> p = *pos;
        Node<T>* new_node = new Node<T>(val);
        new_node->next = p.next;
        p.next = new_node;
        my_size++;
        
        fixup_pointers();

        return iterator(new_node);
    }
    iterator insert_after( const_iterator pos, T&& value ){
        T val = value;
        Node<T> p = *pos;
        Node<T>* new_node = new Node<T>(val);
        new_node->next = p.next;
        p.next = new_node;
        my_size++;

        fixup_pointers();

        return iterator(new_node);
    }
    iterator insert_after( const_iterator pos, size_type count, const T& value ){
        if(count == 0){
            return iterator(pos);
        }
        T val = value;
        Node<T> p = *pos;
        Node<T>* curr = &p;
        for(int i=0; i<count; i++){
            Node<T>* new_node = new Node<T>(val);
            new_node->next = curr->next;
            curr->next = new_node;
            my_size++;
            curr = new_node;
        }

        fixup_pointers();

        return iterator(curr);
    }
    template< class InputIt >
    iterator insert_after( const_iterator pos, InputIt first, InputIt last ){
        if(first==last){
            return iterator(pos);
        }
        Node<T> p = *pos;
        Node<T>* curr = &p;
        InputIt c = first;
        while(c != last){
            Node<T>* new_node = new Node<T>(*c);
            new_node->next = curr->next;
            curr->next = new_node;
            curr = new_node;
            my_size++;
            c++;
        }
        Node<T>* new_node = new Node<T>(*c);
        new_node->next = curr->next;
        curr->next = new_node;
        curr = new_node;
        my_size++;

        fixup_pointers();
        return iterator(curr);
    }
    iterator insert_after( const_iterator pos, std::initializer_list<T> ilist ){
        size_t count = ilist.size();
        if(count == 0){
            return iterator(pos);
        }
        Node<T> p = *pos;
        Node<T>* curr = &p;
        for(int i=0; i<count;i++){
            Node<T>* new_node = new Node<T>(ilist[i]);
            new_node->next = curr->next;
            curr->next = new_node;
            curr = new_node;
            my_size++;
        }
        fixup_pointers();
        return iterator(curr);
    }

    template< class... Args >
    iterator emplace_after( const_iterator pos, Args&&... args );
 
    iterator erase_after( const_iterator pos ){
        if(my_size == 0 || pos == const_iterator(my_tail)){
            return end();
        }
        Node<T> p = *pos;
        Node<T>* temp = p.next;
        p.next = temp->next;
        delete temp;
        my_size--;
        fixup_pointers();
        return iterator(p.next);
    }
    iterator erase_after( const_iterator first, const_iterator last ){
        if(first == last){
            return iterator(last);
        }
        Node<T> f = *first;
        Node<T>* curr = f.next;
        Node<T> l = *last;
        Node<T>* ln = l.next;
        while(curr->next != ln){
            Node<T>* temp = curr;
            curr = curr->next;
            delete temp;
            my_size--;
        }
        f.next = curr;
        fixup_pointers();
        return iterator(curr);
    }

    void push_front( const T& value ){
        Node<T>* new_node = new Node<T>(value);
        //empty list
        if (my_size == 0){
            new_node->next = my_after;
            my_before->next = new_node;
            my_head = new_node;
            my_tail = new_node;
            my_size++;
        }
        //not empty list
        {
            new_node->next = my_head;
            my_before->next = new_node;
            my_head = new_node;
            my_size++;
        }
    }
    void push_front( T&& value ){
        Node<T>* new_node = new Node<T>(value);
        //empty list
        if (my_size == 0){
            new_node->next = my_after;
            my_before->next = new_node;
            my_head = new_node;
            my_tail = new_node;
            my_size++;
        }
        //not empty list
        {
            new_node->next = my_head;
            my_before->next = new_node;
            my_head = new_node;
            my_size++;
        }
    }

    template< class... Args >
    void emplace_front( Args&&... args );

    void pop_front(){
        if(my_size == 0){
            return;
        }
        Node<T>* f = my_front;
        my_before->next = f->next;
        my_front = f->next;
        delete f;
        my_size--;
    }

    void resize( size_type count ){
        if(count == my_size){
            return;
        }
        if(count < my_size){
            Node<T>* curr = my_before;
            for(int i=0; i<count; i++){
                curr = curr->next;
            }
            curr->next = my_after;
            my_tail = curr;
            my_size = count;
            return;
        }
        else{
            size_t diff = count - my_size;
            for(int i=0; i<diff; i++){
                Node<T>* new_node = new Node<T>();
                new_node->next = my_after;
                my_tail->next = new_node;
                my_tail = new_node;
                my_size++;
            }
        }
    }
    void resize( size_type count, const value_type& value ){
        if(count == my_size){
            return;
        }
        if(count < my_size){
            Node<T>* curr = my_before;
            for(int i=0; i<count; i++){
                curr = curr->next;
            }
            curr->next = my_after;
            my_tail = curr;
            my_size = count;
            return;
        }
        else{
            size_t diff = count - my_size;
            for(int i=0; i<diff; i++){
                Node<T>* new_node = new Node<T>(value);
                new_node->next = my_after;
                my_tail->next = new_node;
                my_tail = new_node;
                my_size++;
            }
        }
    }

    void swap( forward_list& other ){
        forward_list temp = *this;
        *this = other;
        other = temp;
    }

};