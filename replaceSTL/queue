#ifndef QUEUE_H_
#define QUEUE_H_

namespace std{
    template <typename T>
    class queue {
    private:

    public:
        // types
        using value_type            = T;
        using size_type             = size_t;

        queue();
        bool                empty() const;
        size_type           size() const;
        value_type&         front();
        const value_type&   front() const;
        value_type&         back();
        const value_type&   back() const;

    };

} // end namespace std


#endif