#pragma once

namespace std{
    template <typename T, unsigned int N>
    class array {
        public:
            using value_type            = T;
            using pointer               = T*;
            using const_pointer         = const T;
            using reference             = T&;
            using const_reference       = const T&;
            using size_type             = size_t;
            
            struct iterator{

            };

            struct const_iterator{

            };

            // CAPACITY
            size_type size() const;
            size_type max_size() const;
            bool empty() const;

        private:
            size_t size;
            T* array;
    };
}





};