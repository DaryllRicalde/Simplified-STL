template <typename T>
class deque {
private:
    T val[200];
    T frnt;
public:

    struct iterator
    {
        using value_type = T;
        using pointer = T; 
        using reference = T&;



        iterator(pointer ptr) : m_ptr(ptr) {}

        pointer operator->() { return m_ptr; }
        reference operator*() {return m_ptr; }

    private:

        pointer m_ptr;
        

    };

    iterator begin() { return iterator(val[0]); }

    deque() {

    }

    void push_back(T a){
      
    }

    T &front() {   
      return frnt;
    }

private:


};